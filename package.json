{
  "name": "visual-zk",
  "private": true,
  "version": "1.0.0",
  "main": "./dist/main/main.js",
  "scripts": {
    "start": "concurrently \"yarn start:main\" \"yarn start:renderer\"",
    "start:main": "electron-webpack dev",
    "start:renderer": "cross-env BROWSER=none APP_ROOT=src/renderer umi dev",
    "build-main-prod": "cross-env NODE_ENV=production webpack --config ./build/webpack.main.prod.config.js",
    "build-main-dev": "cross-env NODE_ENV=development webpack --config ./build/webpack.main.config.js",
    "build:renderer": "cross-env APP_ROOT=src/renderer umi build",
    "pack": "npm run build:renderer && npm run build-main-prod && electron-builder --dir",
    "dist": "electron-builder",
    "dist-mac": "electron-builder -m",
    "dist-win": "electron-builder -w"
  },
  "build": {
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "mac": {
      "category": "your.app.category.type"
    },
    "directories": {
      "output": "release"
    },
    "appId": "com.ghostg00.visual-zk",
    "asar": false
  },
  "dependencies": {
    "dva": "^2.4.1",
    "electron-referer": "^0.3.0",
    "antd": "^3.19.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "nedb": "^1.8.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "@types/nedb": "^1.8.7",
    "babel-eslint": "^9.0.0",
    "cross-env": "^5.2.0",
    "electron": "^4.0.5",
    "electron-builder": "^20.38.5",
    "electron-debug": "^2.0.0",
    "electron-webpack": "^2.6.2",
    "electron-webpack-ts": "^3.1.1",
    "roadhog": "^2.4.2",
    "eslint": "^5.4.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "umi": "^2.7.0",
    "umi-plugin-react": "^1.8.0",
    "umi-types": "^0.3.0",
    "webpack": "4.28.0",
    "webpack-cli": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "tslint": "^5.12.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0"
  },
  "electronWebpack": {
    "main": {
      "sourceDirectory": "src/main"
    },
    "renderer": {
      "sourceDirectory": null
    }
  }
}
