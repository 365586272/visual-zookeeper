{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": false,
    "sourceMap": false,
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    },
    "allowSyntheticDefaultImports": true
  }
}
